N=8
def safe(b,r,c):
    return all(b[i]!=c and b[i]!=c-(r-i) and b[i]!=c+(r-i) for i in range(r))
def solve(r=0,b=[]):
    if r==N:
        for i in range(N):
            print(" ".join("Q" if b[i]==j else "." for j in range(N)))
        print();return
    for c in range(N):
        if safe(b,r,c): solve(r+1,b+[c])
solve()
output
Q . . . . . . .
. . . . Q . . .
. . . . . . . Q
. . . . . Q . .
. . Q . . . . .
. . . . . . Q .
. Q . . . . . .
. . . Q . . . .
